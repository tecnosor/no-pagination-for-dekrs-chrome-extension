{
    "manifest_version": 3,
    "name": "No pagination for Dekrs - Tecnosor Extension",
    "version": "1.1",
    "description": "An extension to show all records on the Dekrs site. Created by Alfonso Soria",
    "permissions": [
    ],
    "content_scripts": [
      {
        "matches": ["https://*.dekrs.com/*"],
        "js": ["magin_button_tecnosor.js"]
      }
    ],
    "action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }